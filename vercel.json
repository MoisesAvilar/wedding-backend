{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/presentes",
      "dest": "/server.js"
    },
    {
      "cors": {
        "origin": "*",
        "methods": ["GET", "POST", "PATCH", "OPTIONS"],
        "allowedHeaders": ["Content-Type"]
      }
    }
  ]
}
